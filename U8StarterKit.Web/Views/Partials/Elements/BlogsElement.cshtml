@inherits Umbraco.Web.Mvc.UmbracoViewPage<BlogsElement>
@using U8StarterKit.Web.Models.Umbraco
@using U8StarterKit.Web.Extensions
@using U8StarterKit.Web.Models.ViewModels
@{
    var blogNode = Model.Blog.Select(x => new Blog(x)).FirstOrDefault();
    var blog = new BlogsViewModel()
    {
        Blog = blogNode,
        BlogsPosts = blogNode.Children.Select(x => new Blogpost(x)).ToList(),
        HasPaging = Model.HasPaging,
        IsHalfWidth = Model.IsHalfWidth,
        ItemsToShow = (int)Model.ItemsToShow
    };
}
@Html.Partial("~/Views/Partials/Blogs.cshtml", blog)
                   