@inherits Umbraco.Web.Mvc.UmbracoViewPage<TitleComponent>
@using U8StarterKit.Web.Models.Umbraco
@using U8StarterKit.Web.Extensions

@if (Model.Title.HasValue())
{
    <h2 class="title is-size-3 is-size-3-tablet">@Model.Title</h2>
}

@if (Model.SubTitle.HasValue())
{
    <div class="subtitle is-size-5 is-size-5-tablet">@Model.SubTitle</div>
}
